{
    "$schema": "http://schema.trogon.eu/json/apidocs.json",
    "name": "Page",
    "description": "Describes single page",
    "introduced": "0.2.0",
    "version": "0.4.0",
    "properties": [
        {
            "name": "blocks",
            "type": "BlockCollection",
            "access": "Read Only",
            "description": "Stores page content as list of block elements"
        },
        {
            "name": "info",
            "type": "PageInfo",
            "access": "Read Only",
            "description": "Stores page meta informations"
        }
    ],
    "methods": [
        {
            "name": "addPagebreak",
            "type": "PageBreak",
            "description": "Append new PageBreak block"
        },
        {
            "name": "addParagraph",
            "type": "Paragraph",
            "description": "Append new Paragraph block"
        },
        {
            "name": "addText",
            "type": "TextBlock",
            "description": "Append new text entry wrapped into TextBlock. Legacy method, use Paragraph instead."
        }
    ]
}